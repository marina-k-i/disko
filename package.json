{
  "name": "disko",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "set": "run-s db-up db-migrate-dev db-seed",
    "dev": "next dev",
    "db-up": "docker-compose up -d",
    "db-seed": "prisma db seed",
    "db-migrate-dev": "prisma migrate dev",
    "build": "next build",
    "vercel-build": "next build && prisma generate",
    "analyze": "ANALYZE=true yarn build",
    "start": "next start",
    "lint": "next lint --dir src",
    "lint:fix": "yarn lint --fix",
    "prisma:generate": "PRISMA_CLIENT_ENGINE_TYPE='dataproxy' prisma generate",
    "prisma:migrate": "DATABASE_URL=\"$(grep MIGRATE_DATABASE_URL .env | cut -d '=' -f2 -f3)\" prisma migrate deploy"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.11",
    "@chakra-ui/react": "^2.3.7",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11",
    "@ffmpeg/ffmpeg": "^0.11.6",
    "@mantine/carousel": "^5.7.1",
    "@mantine/core": "^5.7.1",
    "@mantine/ds": "^5.7.1",
    "@mantine/hooks": "^5.7.1",
    "@mantine/next": "^5.7.1",
    "@mantine/prism": "^5.7.1",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@next/bundle-analyzer": "^13.0.2",
    "@stripe/react-stripe-js": "^1.14.2",
    "@stripe/stripe-js": "^1.42.1",
    "@tabler/icons": "^1.110.0",
    "@types/stripe": "^8.0.417",
    "apexcharts": "^3.36.3",
    "aws-sdk": "^2.1250.0",
    "boring-avatars": "^1.7.0",
    "dexie": "^3.2.2",
    "dotenv": "^16.0.3",
    "embla-carousel-autoplay": "^7.0.5",
    "embla-carousel-react": "^7.0.5",
    "framer-motion": "^7",
    "jotai": "^1.9.1",
    "next": "13.0.2",
    "next-auth": "^4.16.4",
    "next-axiom": "^0.15.1",
    "next-s3-upload": "^0.2.4",
    "npm-run-all": "^4.1.5",
    "react": "18.2.0",
    "react-apexcharts": "^1.4.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.39.1",
    "react-icons": "^4.6.0",
    "react-table": "^7.8.0",
    "stripe": "^10.17.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@prisma/client": "^4.6.0",
    "@types/node": "^18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "@types/uuid": "^8.3.4",
    "eslint": "8.27.0",
    "eslint-config-next": "13.0.2",
    "prettier": "^2.7.1",
    "prisma": "^4.6.0",
    "ts-node": "^10.9.1",
    "typescript": "4.8.4"
  },
  "volta": {
    "node": "16.15.1",
    "yarn": "1.22.19"
  }
}
